{
  "name": "graphql-code-generator-plugin-typescript-typename-typeguards",
  "version": "0.0.2",
  "description": "A plugin for graphql code generator that generate user defined type guards with __typename",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "NODE_ENV=production vitest",
    "dev": "onchange 'src/**/*.ts' -- npm run build",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write .",
    "release": "shipjs prepare"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tyankatsu0105/graphql-code-generator-plugin-typescript-typename-typeguards.git"
  },
  "keywords": [
    "graphql",
    "codegen",
    "typescript",
    "user defined type guards",
    "__typename"
  ],
  "author": "tyankatsu <frips.ryilsufupe+dev@gmail.com> (https://tyankatsu.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tyankatsu0105/graphql-code-generator-plugin-typescript-typename-typeguards/issues"
  },
  "homepage": "https://github.com/tyankatsu0105/graphql-code-generator-plugin-typescript-typename-typeguards#readme",
  "devDependencies": {
    "@graphql-codegen/plugin-helpers": "5.0.1",
    "@graphql-codegen/visitor-plugin-common": "4.0.1",
    "@graphql-tools/graphql-file-loader": "8.0.0",
    "@graphql-tools/load": "8.0.0",
    "@types/node": "20.4.10",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "eslint": "8.47.0",
    "eslint-import-resolver-typescript": "3.6.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "onchange": "7.1.0",
    "prettier": "3.0.1",
    "shipjs": "0.26.3",
    "typescript": "5.1.6",
    "vitest": "0.34.1"
  },
  "dependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "peerDependencies": {
    "graphql": "^15.0.0 || ^16.0.0"
  },
  "engines": {
    "node": "^16 || ^18 || ^19 || ^20"
  }
}
